<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkAdjustment" id="adjustment1">
    <property name="upper">10000</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adjustment2">
    <property name="upper">10000</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkWindow" id="templateDialog">
    <property name="title" translatable="1">Configure new page template</property>
    <property name="modal">1</property>
    <property name="destroy-with-parent">1</property>
    <property name="child">
      <object class="GtkBox">
        <property name="margin-start">10</property>
        <property name="margin-end">10</property>
        <property name="margin-top">10</property>
        <property name="margin-bottom">10</property>
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkLabel" id="label1">
            <property name="label" translatable="1">These settings will be used for new pages</property>
            <property name="use-markup">1</property>
            <property name="xalign">0</property>
          </object>
        </child>
        <child>
          <object class="GtkGrid" id="grid1">
            <property name="vexpand">1</property>
            <property name="halign">center</property>
            <property name="margin-top">12</property>
            <property name="column-spacing">10</property>
            <child>
              <object class="GtkLabel" id="label2">
                <property name="label" translatable="1">&lt;b&gt;Page size&lt;/b&gt;</property>
                <property name="use-markup">1</property>
                <property name="ellipsize">middle</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="lbPageSize">
                <property name="name">lbPageSize</property>
                <property name="label">10cm x 20cm</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="column">1</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="btChangePaperSize">
                <property name="label" translatable="1">Change</property>
                <property name="name">btChangePaperSize</property>
                <property name="focusable">1</property>
                <layout>
                  <property name="column">2</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkColorButton" id="cbBackgroundButton">
                <property name="name">cbBackgroundButton</property>
                <property name="focusable">1</property>
                <property name="title" translatable="1">Select Background Color</property>
                <layout>
                  <property name="column">1</property>
                  <property name="row">3</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="cbCopyLastPage">
                <property name="label" translatable="1">Copy current page background instead</property>
                <property name="name">cbCopyLastPage</property>
                <property name="focusable">1</property>
                <property name="tooltip-text" translatable="1">Copy the background of the current page instead of using the background set above.</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">4</property>
                  <property name="column-span">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="box1">
                <child>
                  <object class="GtkLabel" id="lbBackgroundType">
                    <property name="hexpand">1</property>
                    <property name="name">lbBackgroundType</property>
                    <property name="label">BACKGROUND_TYPE</property>
                    <property name="xalign">0</property>
                  </object>
                </child>
                <child>
                  <object class="GtkMenuButton" id="btBackgroundDropdown">
                    <property name="name">btBackgroundDropdown</property>
                    <property name="focusable">1</property>
                    <property name="focus-on-click">0</property>
                    <child>
                      <placeholder/>
                    </child>
                  </object>
                </child>
                <layout>
                  <property name="column">1</property>
                  <property name="row">2</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="label4">
                <property name="label" translatable="1">&lt;b&gt;Background Color&lt;/b&gt;</property>
                <property name="use-markup">1</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="label3">
                <property name="label" translatable="1">&lt;b&gt;Background&lt;/b&gt;</property>
                <property name="use-markup">1</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="cbCopyLastPageSize">
                <property name="label" translatable="1">Copy current page size instead</property>
                <property name="name">cbCopyLastPageSize</property>
                <property name="focusable">1</property>
                <property name="tooltip-text" translatable="1">Use the current page size instead of the page size above.</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                  <property name="column-span">3</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <property name="margin-top">6</property>
            <property name="spacing">4</property>
            <property name="homogeneous">1</property>
            <child>
              <object class="GtkButton" id="btSave">
                <property name="hexpand">1</property>
                <property name="label" translatable="1">Save to file</property>
                <property name="name">btSave</property>
                <property name="focusable">1</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="btLoad">
                <property name="hexpand">1</property>
                <property name="label" translatable="1">Load from file</property>
                <property name="name">btLoad</property>
                <property name="focusable">1</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="btCancel">
                <property name="hexpand">1</property>
                <property name="label" translatable="1">Cancel</property>
                <property name="name">btCancel</property>
                <property name="focusable">1</property>
                <child>
                  <object class='GtkShortcutController'>
                    <property name='scope'>managed</property>
                    <child>
                      <object class='GtkShortcut'>
                        <property name='trigger'>Escape</property>
                        <property name='action'>activate</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="btOk">
                <property name="hexpand">1</property>
                <property name="label" translatable="1">Ok</property>
                <property name="name">btOk</property>
                <property name="focusable">1</property>
                <property name="receives-default">1</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </property>
  </object>
</interface>
